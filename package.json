{
  "name": "cube-budget-ui",
  "version": "0.0.3",
  "description": "ui library for cube-budget project",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "react-scripts start",
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "test": "jest",
    "test:watch": "jest --watchAll --verbose",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "prepare": "husky",
    "lint": "eslint ./src/ --ext .ts,.tsx",
    "lint:fix": "eslint ./src --ext .ts,.tsx --fix",
    "release": "standard-version"
  },
  "jest": {
    "testEnvironment": "jsdom"
  },
  "babel": {
    "sourceType": "unambiguous",
    "presets": [
      "@babel/preset-env",
      "@babel/preset-typescript",
      [
        "@babel/preset-react",
        {
          "runtime": "automatic"
        }
      ]
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JangAyeon/Cube-UI.git"
  },
  "homepage": "https://github.com/JangAyeon/Cube-UI#readme",
  "bugs": {
    "url": "https://github.com/JangAyeon/Cube-UI/issues"
  },
  "author": "puuupuuu",
  "license": "ISC",
  "devDependencies": {
    "@babel/preset-env": "^7.23.9",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@chromatic-com/storybook": "^3.2.2",
    "@commitlint/cli": "^18.6.0",
    "@commitlint/config-conventional": "^18.6.0",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "@storybook/addon-essentials": "^8.5.0-alpha.14",
    "@storybook/addon-links": "^8.5.0-alpha.14",
    "@storybook/addon-onboarding": "^8.5.0-alpha.14",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/blocks": "^8.5.0-alpha.14",
    "@storybook/experimental-addon-test": "^8.5.0-alpha.14",
    "@storybook/react": "^8.5.0-alpha.14",
    "@storybook/react-vite": "^8.5.0-alpha.14",
    "@storybook/test": "^8.5.0-alpha.14",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@types/jest": "^29.5.12",
    "@types/react": "^18.2.55",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@vitest/browser": "^2.1.6",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.56.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-storybook": "^0.11.1",
    "husky": "^9.0.10",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "playwright": "^1.49.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup-plugin-dts": "^6.1.0",
    "rollup-plugin-postcss": "^4.0.2",
    "standard-version": "^9.5.0",
    "storybook": "^8.5.0-alpha.14",
    "tailwindcss": "^3.4.15",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3",
    "vitest": "^2.1.6"
